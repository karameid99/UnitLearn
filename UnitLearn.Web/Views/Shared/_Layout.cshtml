    <script src="~/HomeTemplate/assets/js/liquidAjaxMailchimp.min.js"></script>

    <script async src="~/HomeTemplate/assets/vendors/modernizr.min.js"></script>

    <script src="~/lib/jquery/dist/jquery.js"></script>

    @RenderSection("Scripts", false)

    <script>
        $('.error-message').hide();
        $('.sent-message').hide();
        $(document).on("click", "#contactUsSubmit", function (e) {
            e.preventDefault();
            var form = jQuery('#contactform')[0];
            var data = new FormData(form);
            jQuery.ajax({
                type: 'POST',
                url: '/Home/Contactus',
                data: data,
                cache: false,
                contentType: false,
                processData: false,
                success: function (response) {
                    if (response == "success") {
                        $('.sent-message').show();
                        $('.error-message').hide();
                    }
                    else {
                        $('.error-message').show();
                        $('.sent-message').hide();
                    }
                },
                error: function (jqXHR, error, errorThrown) {
                    $('.error-message').show();
                    $('.sent-message').hide();
                }
            });
        });

    </script>
